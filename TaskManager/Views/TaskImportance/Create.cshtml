@using EntityLayer.Concrete
@model TaskImportance

<div>
    <h3 class="mb-0">Görevlere Önem Seviyesi Ekleme</h3>
</div>
@using (Html.BeginForm("Create", "TaskImportance", FormMethod.Post))
{

    @Html.Label("Başlığı")
    @Html.TextBoxFor(ba => ba.TaskImportanceName, new { @class = "form-control" })
    @Html.ValidationMessageFor(ba => ba.TaskImportanceName)


    <button class="btn btn-info">Kaydet</button>
}