@using EntityLayer.Concrete
@model List<TaskStatuse>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Name</th>                    
            <th>Sil</th>
            <th>Güncelle</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {


            <tr>
                <td>@item.TaskStatuseName</td>

                
                <td><a href="~/TaskStatuse/Delete/@item.TaskStatuseID" class="btn btn-outline-danger">Sil</a></td>
                <td><a href="~/TaskStatuse/Details/@item.TaskStatuseID" class="btn btn-outline-danger">Güncelle</a></td>
            </tr>
        }
    </tbody>

</table>
<a href="~/TaskStatuse/Create/" class="btn btn-outline-danger">Ekle</a>