@using EntityLayer.Concrete
@model List<Day>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Gün</th>
            <th>İçerik</th>
            <th>Bitiş</th>
            <th>Sil</th>
            <th>Güncelle</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {


            <tr>
                <td>@item.DayTime</td>

                <td>@item.DayTaskContent</td>

                <td>@item.DayTaskFinishTime</td>
                <td><a href="~/Day/Delete/@item.DayId" class="btn btn-outline-danger">Sil</a></td>
                <td><a href="~/Day/Details/@item.DayId" class="btn btn-outline-danger">Güncelle</a></td>
            </tr>
        }
    </tbody>

</table>
<a href="~/Day/Create/" class="btn btn-outline-danger">Ekle</a>